{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist-social",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "cli/**/*.ts",
    "core/**/*.ts",
    "executors/**/*.ts",
    "utils/**/*.ts",
    "tests/**/*.ts"
  ]
}

