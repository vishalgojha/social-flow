{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ParsedIntent",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "action",
    "params"
  ],
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "onboard",
        "doctor",
        "status",
        "config",
        "logs",
        "replay",
        "get_profile",
        "create_post",
        "list_ads",
        "get_status",
        "unknown"
      ]
    },
    "params": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    }
  }
}
